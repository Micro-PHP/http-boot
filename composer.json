{
    "name": "micro/plugin-http-boot",
    "description": "Micro Framework: Lightweight HTTP router",
    "license": "MIT",
    "type": "micro-plugin",
    "authors": [
        {
            "name": "Stanislau Komar",
            "email": "kost@micro-php.net"
        }
    ],
    "require": {
        "micro/kernel-app": "^2.0",
        "micro/kernel-boot-plugin-depended": "^2.0",
        "micro/plugin-event-emitter": "^2.0",
        "micro/plugin-http-core": "^2.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.34"
    },
    "autoload": {
        "psr-4": {
            "Micro\\Plugin\\HttpBoot\\": "/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
